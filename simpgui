-- Scripts:

local function SPJEOBX_fake_script() -- ButSheRejectedMe.MainScript 
	local script = Instance.new('Script', ButSheRejectedMe)


	coroutine.resume(coroutine.create(function()
		while true do
			wait()
			piss = script.Parent.WhoToHeartbreak.Text
		end
	end))

	local function onClicked()
		local iaminlove = math.random(1,10)
		if iaminlove == 1 then
			script.Parent.TextLabel.Text = thingtable.PersonToSimpFor .. " is hot"
		elseif iaminlove == 2 then
			script.Parent.TextLabel.Text = thingtable.PersonToSimpFor .. " is beautiful"
		elseif iaminlove == 3 then
			script.Parent.TextLabel.Text = thingtable.PersonToSimpFor .. " is my god"
		elseif iaminlove == 4 then
			script.Parent.TextLabel.Text = thingtable.PersonToSimpFor .. " is perfect"
		elseif iaminlove == 5 then
			script.Parent.TextLabel.Text = thingtable.PersonToSimpFor .. " is poggers"
		elseif iaminlove == 6 then
			script.Parent.TextLabel.Text = thingtable.PersonToSimpFor .. " is not as hot as arden but close"
		elseif iaminlove == 7 then
			script.Parent.TextLabel.Text = thingtable.PersonToSimpFor .. " is pretty"
		elseif iaminlove == 8 then
			script.Parent.TextLabel.Text = thingtable.PersonToSimpFor .. " is fine for a gremlin"
		elseif iaminlove == 9 then
			script.Parent.TextLabel.Text = thingtable.PersonToSimpFor .. ", do that thang fart??"
		elseif iaminlove == 10 then
			script.Parent.TextLabel.Text = thingtable.PersonToSimpFor .. " is sweet"
		end
	end

	script.Parent.ChooseButton.MouseButton1Down:connect(onClicked)

end
coroutine.wrap(SPJEOBX_fake_script)()
local function DIJZA_fake_script() -- ButSheRejectedMe.DragSript 
	local script = Instance.new('LocalScript', ButSheRejectedMe)

	local UserInputService = game:GetService("UserInputService")

	local gui = script.Parent

	local dragging
	local dragInput
	local dragStart
	local startPos

	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end

	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position

			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)

	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)

	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(DIJZA_fake_script)()
